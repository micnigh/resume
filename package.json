{
  "name": "mn-resume",
  "description": "",
  "version": "2.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "babel-node --extensions '.js,.jsx,.ts,.tsx' node_modules/gatsby/dist/bin/gatsby.js  build",
    "build:pdf": "babel-node --extensions '.js,.jsx,.ts,.tsx' scripts/buildPdf/index.ts --exec",
    "develop": "babel-node --extensions '.js,.jsx,.ts,.tsx' node_modules/gatsby/dist/bin/gatsby.js develop",
    "serve": "serve public",
    "deploy": "babel-node --extensions '.js,.jsx,.ts,.tsx' node_modules/gatsby/dist/bin/gatsby.js build --prefix-paths && gh-pages -d public",
    "deploy:ci": "babel-node --extensions '.js,.jsx,.ts,.tsx' node_modules/gatsby/dist/bin/gatsby.js build --prefix-paths",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.tsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
