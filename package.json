{
  "name": "mn-resume",
  "description": "",
  "version": "2.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "ts-node --fast -r babel-core/register node_modules/gatsby/dist/gatsby-cli.js build",
    "build:pdf": "ts-node --fast -r babel-core/register scripts/buildPdf/index.tsx --exec",
    "develop": "ts-node --fast -r babel-core/register node_modules/gatsby/dist/gatsby-cli.js develop",
    "serve": "serve public",
    "deploy": "ts-node --fast -r babel-core/register node_modules/gatsby/dist/gatsby-cli.js build --prefix-paths && gh-pages -d public",
    "deploy:ci": "ts-node --fast -r babel-core/register node_modules/gatsby/dist/gatsby-cli.js build --prefix-paths",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.tsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "devicon": "^2.0.0",
    "enquire.js": "^2.1.6",
    "express": "^4.15.5",
    "gatsby": "^1.9.45",
    "gatsby-link": "^1.6.17",
    "gatsby-plugin-react-helmet": "^1.0.6",
    "gatsby-plugin-styled-components": "^1.0.5",
    "gatsby-plugin-typescript": "^1.4.8",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "normalize.css": "^7.0.0",
    "normalizr": "^3.2.3",
    "puppeteer": "^0.11.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "serve": "^6.1.0",
    "styled-components": "^2.1.2",
    "svg-react-loader": "^0.4.5",
    "tippy.js": "^1.2.1",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3",
    "webfontloader": "^1.6.28",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/jest": "^21.1.1",
    "@types/lodash": "^4.14.76",
    "@types/marked": "^0.3.0",
    "@types/mkdirp": "^0.5.1",
    "@types/node": "^8.0.31",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/react-helmet": "^5.0.3",
    "@types/react-redux": "^5.0.9",
    "@types/react-router-dom": "^4.0.8",
    "@types/redux-logger": "^3.0.4",
    "@types/uuid": "^3.4.2",
    "gh-pages": "^1.0.0",
    "jest": "^21.2.0",
    "prettier": "^1.7.0",
    "svg-icon": "^0.8.2",
    "ts-jest": "^21.0.1",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0"
  }
}
