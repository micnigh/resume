    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@reach/router": "^1.1.1",
    "@types/puppeteer": "^1.6.1",
    "@types/yargs": "^11.1.1",
    "babel-plugin-styled-components": "^1.6.0",
    "devicon": "^2.0.0",
    "enquire.js": "^2.1.6",
    "express": "^4.16.3",
    "gatsby": "next",
    "gatsby-link": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-styled-components": "next",
    "gatsby-plugin-typescript": "next",
    "gatsby-source-filesystem": "next",
    "lodash": "^4.17.13",
    "marked": "^0.5.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.0",
    "normalizr": "^3.2.4",
    "puppeteer": "^1.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "serve": "^10.0.0",
    "styled-components": "^3.4.5",
    "svg-react-loader": "^0.4.5",
    "tippy.js": "^2.5.4",
    "ts-loader": "^4.5.0",
